package com.zhaoguan.mpluslibs;

import java.util.regex.Pattern;

/**
 * Created by husong on 2017/7/7.
 */

public class RegexUtils {

    private static final String IS_PERIOD = "^([0-1]?[0-9]|2[0-3]):([0-5][0-9])-([0-1]?[0-9]|2[0-3]):([0-5][0-9])$";

    public static boolean isPeriod(String input){
        return Pattern.matches(IS_PERIOD, input);
    }
}
